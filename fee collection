pragma solidity ^0.8.7;

contract Feecollection{

    string public name;
    uint256 public roll_no;

    constructor (string memory initialName) {
        name = initialName;

    }

function addName(string memory newName) public {
    name = newName ;

}

function addroll_no(uint256  newroll_no) public {
    roll_no = newroll_no ;
}
}
